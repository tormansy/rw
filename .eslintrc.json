{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "ignorePatterns": ["bundle.js"],
  "rules": {
    "prettier/prettier": "error",
    "camelcase": ["error"],
    "eqeqeq": ["error", "always"]
  }
}
